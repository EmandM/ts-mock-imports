{
  "name": "ts-mock-imports",
  "version": "1.3.16-rc.3",
  "description": "Intuitive mocking for Typescript class imports",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmandM/ts-mock-imports.git"
  },
  "keywords": [
    "import",
    "mock",
    "stub",
    "typescript",
    "dependency",
    "injection",
    "di",
    "require",
    "test",
    "sinon",
    "node"
  ],
  "author": "Emma McMillan <emmalicious@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmandM/ts-mock-imports/issues"
